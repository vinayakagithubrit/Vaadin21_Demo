#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regexpu-core@6.2.0/node_modules/regexpu-core/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regexpu-core@6.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regexpu-core@6.2.0/node_modules/regexpu-core/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regexpu-core@6.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regjsparser@0.12.0/node_modules/regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../../../regjsparser@0.12.0/node_modules/regjsparser/bin/parser" "$@"
fi
