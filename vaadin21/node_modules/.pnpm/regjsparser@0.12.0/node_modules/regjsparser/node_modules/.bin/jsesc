#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules/jsesc/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules/jsesc/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules/jsesc/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules/jsesc/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/jsesc@3.0.2/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules/regjsparser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/regjsparser@0.12.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsesc@3.0.2/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../jsesc@3.0.2/node_modules/jsesc/bin/jsesc" "$@"
fi
