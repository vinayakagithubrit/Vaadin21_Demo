#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules/copy-concurrently/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/mkdirp@0.5.6/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules/copy-concurrently/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/copy-concurrently@1.0.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.6/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.6/node_modules/mkdirp/bin/cmd.js" "$@"
fi
