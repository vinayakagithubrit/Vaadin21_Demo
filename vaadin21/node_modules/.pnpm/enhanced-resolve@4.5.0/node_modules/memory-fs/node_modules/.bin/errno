#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/memory-fs@0.5.0/node_modules/memory-fs/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/memory-fs@0.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/errno@0.1.8/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/memory-fs@0.5.0/node_modules/memory-fs/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/memory-fs@0.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
else
  exec node  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
fi
