#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babel-traverse@6.26.0/node_modules/babel-traverse/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babel-traverse@6.26.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules/babylon/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babylon@6.18.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babel-traverse@6.26.0/node_modules/babel-traverse/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/babel-traverse@6.26.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
else
  exec node  "$basedir/../../../../../babylon@6.18.0/node_modules/babylon/bin/babylon.js" "$@"
fi
