#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules/terser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules/terser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules/terser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules/terser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/terser@4.8.1/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@4.8.1/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@4.8.1/node_modules/terser/bin/terser" "$@"
fi
