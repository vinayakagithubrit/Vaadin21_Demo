#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules/semver/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules/semver/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/cross-spawn@6.0.6/node_modules/cross-spawn/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/cross-spawn@6.0.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules/semver/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules/semver/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@5.7.2/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/cross-spawn@6.0.6/node_modules/cross-spawn/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/cross-spawn@6.0.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@5.7.2/node_modules/semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../../../semver@5.7.2/node_modules/semver/bin/semver" "$@"
fi
