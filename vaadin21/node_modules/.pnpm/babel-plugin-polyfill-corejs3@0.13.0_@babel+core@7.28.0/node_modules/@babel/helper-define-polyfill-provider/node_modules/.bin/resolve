#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules/resolve/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules/resolve/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules/@babel/helper-define-polyfill-provider/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules/resolve/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules/resolve/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/resolve@1.22.10/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules/@babel/helper-define-polyfill-provider/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-define-polyfill-provider@0.6.5_@babel+core@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../resolve@1.22.10/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../../../resolve@1.22.10/node_modules/resolve/bin/resolve" "$@"
fi
