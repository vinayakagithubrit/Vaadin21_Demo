#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules/semver/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules/semver/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules/@babel/helper-create-regexp-features-plugin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules/semver/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules/semver/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/semver@6.3.1/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules/@babel/helper-create-regexp-features-plugin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/helper-create-regexp-features-plugin@7.27.1_@babel+core@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../semver@6.3.1/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../../../semver@6.3.1/node_modules/semver/bin/semver.js" "$@"
fi
