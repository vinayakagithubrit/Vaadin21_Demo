#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/parser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/parser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules/@babel/template/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/parser/bin/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/parser/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/parser@7.28.0/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/node_modules:/mnt/d/vaadin21/node_modules:/mnt/d/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules/@babel/template/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules/@babel/node_modules:/mnt/d/vaadin21/node_modules/.pnpm/@babel/template@7.27.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../parser@7.28.0/node_modules/@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../../../../../../parser@7.28.0/node_modules/@babel/parser/bin/babel-parser.js" "$@"
fi
